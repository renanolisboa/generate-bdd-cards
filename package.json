{
  "name": "jira-gdocs-gepeto-cli",
  "version": "1.0.0",
  "description": "CLI workflow for creating Jira issues from Google Docs using Gepeto for BDD card generation",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "tsx src/test.ts",
    "test:integration": "tsx src/integration-test.ts",
    "test:mock": "tsx src/mock-integration-test.ts",
    "test:gepeto": "tsx src/test-gepeto-only.ts",
    "diagnose": "tsx src/diagnose-apis.ts",
    "debug:gepeto": "tsx src/debug-gepeto.ts"
  },
  "bin": {
    "jira-gdocs-gepeto": "dist/index.js"
  },
  "keywords": ["jira", "google-docs", "bdd", "cli", "automation"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "googleapis": "^128.0.0",
    "zod": "^3.22.4",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "@types/inquirer": "^9.0.7"
  }
}
